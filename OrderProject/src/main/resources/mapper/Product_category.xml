<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.order.dao.Product_categoryMapper">
    <select id="selectProduct_categorys" parameterType="fenye" resultType="product_category">
        select * from product_category limit #{page},#{pageSize}
    </select>
    <select id="selectProduct_categoryCount" parameterType="fenye" resultType="int">
        select count(category_id) from product_category
    </select>
</mapper>